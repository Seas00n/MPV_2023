#ifndef INC_RINGBUFFER_H_
#define INC_RINGBUFFER_H_
#define Buffer_MAX 80
typedef struct{
	unsigned char headPosition;
	unsigned char tailPosition;
	unsigned char ringBuf[Buffer_MAX];
}ringBuffer_t;



#endif /* INC_RINGBUFFER_H_ */
