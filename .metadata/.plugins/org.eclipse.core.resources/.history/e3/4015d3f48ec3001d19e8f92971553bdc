#ifndef INC_MSG_H_
#define INC_MSG_H_
typedef struct{
	uint8_t head;
	uint8_t end;
	int16_t value1;
	int16_t value2;
	int16_t value3;
	int16_t value4;
	int16_t value5;
	int16_t value6;
}M2P;
typedef struct{
	uint8_t head;
	uint8_t id;
	int16_t value1;
	int16_t value2;
	int16_t value3;
	int16_t value4;
	uint8_t ext_value;
	uint8_t end;
}P2M;
uint32_t size_m2p = 14;
uint32_t size_p2m = 12;
void p2mmsg_memcpy(uint8_t*,volatile P2M);
void m2pmsg_memcpy(uint8_t*,volatile M2P);
#endif /* INC_MSG_H_ */
